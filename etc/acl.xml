<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Osc_Sample::Category" title="Category" sortOrder="10">
                    <resource id="Osc_Sample::Category_save" title="Save Category" sortOrder="10"/>
                    <resource id="Osc_Sample::Category_delete" title="Delete Category" sortOrder="20"/>
                    <resource id="Osc_Sample::Category_update" title="Update Category" sortOrder="30"/>
                    <resource id="Osc_Sample::Category_view" title="View Category" sortOrder="40"/>
                </resource>
                <resource id="Osc_Sample::SubCategory" title="SubCategory" sortOrder="10">
                    <resource id="Osc_Sample::SubCategory_save" title="Save SubCategory" sortOrder="10"/>
                    <resource id="Osc_Sample::SubCategory_delete" title="Delete SubCategory" sortOrder="20"/>
                    <resource id="Osc_Sample::SubCategory_update" title="Update SubCategory" sortOrder="30"/>
                    <resource id="Osc_Sample::SubCategory_view" title="View SubCategory" sortOrder="40"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
